<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="ZTalimCRM">
        <addColumn tableName="SCHEDULE">
            <column name="END_TIME" type="DATETIME"/>
        </addColumn>

        <addNotNullConstraint columnName="END_TIME" defaultNullValue="now" tableName="SCHEDULE"/>
    </changeSet>
    <changeSet id="2" author="ZTalimCRM">
        <addColumn tableName="SCHEDULE">
            <column name="START_TIME" type="DATETIME"/>
        </addColumn>

        <addNotNullConstraint columnName="START_TIME" defaultNullValue="now" tableName="SCHEDULE"/>
    </changeSet>
</databaseChangeLog>